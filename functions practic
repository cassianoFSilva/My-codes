{
function travel(distance, speed) {
    let time = distance / speed;
    let fuel = distance / 12;

    return {
        time: time,
        fuel: fuel
    };
}

let result = travel(150, 50);

console.log(result.time);
console.log(result.fuel);
}

{
function circle(radius){
    let area = Math.PI * radius * radius
    let circumference = 2 * Math.PI * radius

    return{area, circumference};
}

let result = circle(60);

console.log("area is: ")
console.log(result.area.toFixed(2));
console.log("circumference is: ")
console.log(result.circumference.toFixed(2));
}

{
function temperature(celsius){
    let fahrenheit = celsius * 9/5 + 32
    let kelvin = celsius + 273.15

    return{fahrenheit, kelvin };
}
    let result = temperature(125);
    
    console.log("fahrenheit is: ")
    console.log(result.fahrenheit);
    console.log("kelvin is: ")
    console.log(result.kelvin);
}

{
function salary(workhours, hourvalue){
    let total = workhours * hourvalue

    return{total}
}
    let result = salary(50, 15);

    console.log(result.total);
}
